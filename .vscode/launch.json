{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "[debug] App",
      "type": "dart",
      "request": "launch",
      "cwd": "${workspaceFolder}",
      "args": [
        "--dart-define-from-file=config/development.json",
      ],
      "env": {}
    },
    {
      "name": "[debug] App (Drop database)",
      "type": "dart",
      "request": "launch",
      "cwd": "${workspaceFolder}",
      "args": [
        "--dart-define-from-file=config/development.json",
        "--dart-define=DROP_DATABASE=true"
      ],
      "env": {}
    },
    {
      "name": "[debug] App (Memmory database)",
      "type": "dart",
      "request": "launch",
      "cwd": "${workspaceFolder}",
      "args": [
        "--dart-define-from-file=config/development.json",
        "--dart-define=IN_MEMORY_DATABASE=true"
      ],
      "env": {}
    },
    {
      "name": "[debug] Web Server (HTML)",
      "type": "dart",
      "request": "launch",
      "cwd": "${workspaceFolder}",
      "args": [
        "--dart-define-from-file=config/development.json",
        "--device-id=web-server",
        "--web-port=3000",
        "--web-renderer=html",
      ],
      "env": {}
    }
  ]
}